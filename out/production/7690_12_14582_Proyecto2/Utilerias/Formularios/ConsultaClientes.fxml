<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="554.0" prefWidth="1019.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GestionFormularios.ControladorDeEventos">
   <children>
      <AnchorPane layoutX="1.0" layoutY="1.0" prefHeight="550.0" prefWidth="1013.0" style="-fx-background-color: #414141;">
         <children>
            <Label layoutX="21.0" layoutY="28.0" prefHeight="44.0" prefWidth="171.0" text="Id" textFill="#fcf7f7">
               <font>
                  <Font name="Calibri Bold" size="14.0" />
               </font></Label>
            <Label layoutX="21.0" layoutY="72.0" prefHeight="44.0" prefWidth="171.0" text="NIT" textFill="#fcf7f7">
               <font>
                  <Font name="Calibri Bold" size="14.0" />
               </font></Label>
            <TextField fx:id="txtIDCliente" layoutX="84.0" layoutY="38.0" onKeyTyped="#ActivadorValidarIngresoIDCliente" promptText="ID Cliente" />
            <TextField fx:id="txtNITConsulta" layoutX="84.0" layoutY="82.0" />
            <TableView fx:id="tablaClientes" layoutX="20.0" layoutY="124.0" prefHeight="408.0" prefWidth="973.0">
              <columns>
                <TableColumn fx:id="columid" prefWidth="75.0" text="ID" />
                  <TableColumn fx:id="columnNIT" prefWidth="75.0" text="NIT" />
                  <TableColumn fx:id="columDPI" prefWidth="107.0" text="DPI" />
                  <TableColumn fx:id="columNombre" prefWidth="195.0" text="Nombre" />
                  <TableColumn fx:id="columFechaNac" prefWidth="117.0" text="Fecha Nacimiento" />
                  <TableColumn fx:id="columnGenero" minWidth="0.0" prefWidth="64.0" text="Genero" />
                  <TableColumn fx:id="columEstadoCivil" text="Estado Civil" />
                  <TableColumn fx:id="columnRazonSocial" prefWidth="163.0" text="Razon Social" />
                  <TableColumn fx:id="columnContacto" prefWidth="101.0" text="Contacto" />
              </columns>
            </TableView>
            <Button fx:id="BotonGenerarTabla" layoutX="715.0" layoutY="48.0" mnemonicParsing="false" onAction="#MostraContenidoClientes" prefHeight="25.0" prefWidth="171.0" text="Mostrar Contenido" />
            <Button fx:id="BotonSalirFormConsultaCliente" layoutX="900.0" layoutY="17.0" mnemonicParsing="false" onAction="#SalidaConsultaClientes" prefHeight="51.0" prefWidth="59.0" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="46.0" fitWidth="53.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Imagenes/Exit.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="BotonFormIngresoClientes" layoutX="715.0" layoutY="16.0" mnemonicParsing="false" onAction="#LlamarFormIngresoClientes" prefHeight="25.0" prefWidth="171.0" text="Opcion para Ingresar Clientes" />
            <Button fx:id="BotonBuscarCliente" layoutX="271.0" layoutY="83.0" mnemonicParsing="false" onAction="#BuscarClientes" prefHeight="25.0" prefWidth="86.0" text="Buscar" />
            <Button layoutX="715.0" layoutY="80.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="171.0" text="Eliminar" />
            <Label fx:id="MensajeValidacionIDCliente" layoutX="240.0" layoutY="42.0" prefHeight="17.0" prefWidth="62.0" textFill="WHITE">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</Pane>
