<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GestionFormularios.GestionVistaPrincipal">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="237.0">
        <menus>
          <Menu mnemonicParsing="false" text="Archivo">
            <items>
              <MenuItem fx:id="SelectMenuIngresoClientes" mnemonicParsing="false" onAction="#OpcionMenuIngresoClientes" text="Ingreso Clientes" />
                <MenuItem fx:id="SelectMenuIngresoProductos" mnemonicParsing="false" onAction="#OpcionMenuIngresoProducto" text="Ingreso Productos" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Consultas">
            <items>
              <MenuItem fx:id="consultaClientes" mnemonicParsing="false" onAction="#OpcionConsultaClientes" text="Consultar Clientes" />
                  <MenuItem fx:id="ConsultaProductos" mnemonicParsing="false" onAction="#OpcionConsultaProductos" text="Consultar Productos" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</Pane>
